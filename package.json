{
  "name": "arwes",
  "version": "1.0.0-alpha.2",
  "description": "Front-end framework to build web apps user interfaces based on sci-fi and futuristic concepts",
  "keywords": [
    "gui",
    "ui",
    "sci-fi",
    "futuristic",
    "browser",
    "prhone"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romelperez/arwes.git"
  },
  "bugs": {
    "url": "https://github.com/romelperez/arwes/issues"
  },
  "author": "Romel PÃ©rez (romelperez.com)",
  "license": "MIT",
  "homepage": "https://github.com/romelperez/arwes",
  "main": "dist/index.js",
  "scripts": {
    "test": "TRAVIS=true npm run test-lint && TRAVIS=true npm run test-unit",
    "test-lint": "eslint --cache --quiet ./js",
    "test-unit": "karma start karma.conf.js",
    "bundle": "rimraf ./dist/* && npm run bundle-js && npm run bundle-css",
    "bundle-js": "NODE_ENV=production babel src --out-dir dist --ignore index.test.js",
    "bundle-css": "NODE_ENV=production node build-css.js",
    "docs-server": "node docs.js",
    "docs-build": "npm run docs-js && npm run docs-css",
    "docs-js": "NODE_ENV=production webpack --config webpack.docs.js",
    "docs-js:watch": "webpack --config webpack.docs.js -w",
    "docs-css": "NODE_ENV=production gulp --gulpfile gulpfile.docs.js sass",
    "docs-css:watch": "gulp --gulpfile gulpfile.docs.js"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "doc-ready": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "foundation-sites": "^6.3.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^2.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.7",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "postcss": "^5.2.16",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "webpack": "^1.14.0"
  }
}
