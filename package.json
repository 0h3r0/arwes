{
  "name": "arwes",
  "version": "1.0.0-alpha.2",
  "description": "Sci-Fi Graphical User Interface Framework",
  "keywords": [
    "gui",
    "ui",
    "sci-fi",
    "futuristic",
    "browser",
    "prhone"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romelperez/arwes.git"
  },
  "bugs": {
    "url": "https://github.com/romelperez/arwes/issues"
  },
  "author": "Romel PÃ©rez (romelperez.com)",
  "license": "MIT",
  "homepage": "https://github.com/romelperez/arwes",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run test-lint && npm run test-unit",
    "test-lint": "eslint --cache --quiet ./js",
    "test-unit": "karma start karma.conf.js",
    "docs": "styleguidist build",
    "bundle": "rimraf ./dist/* && npm run bundle-js && npm run bundle-css",
    "bundle-js": "NODE_ENV=production babel src --out-dir dist --ignore index.test.js",
    "bundle-css": "NODE_ENV=production node build-css.js",
    "demos": "node demos.js",
    "demos-build": "npm run demos-js && npm run demos-css",
    "demos-js": "NODE_ENV=production webpack --config webpack.demos.js",
    "demos-js:watch": "webpack --config webpack.demos.js -w",
    "demos-css": "NODE_ENV=production gulp --gulpfile gulpfile.demos.js sass",
    "demos-css:watch": "gulp --gulpfile gulpfile.demos.js"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "doc-ready": "^1.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "chai": "^3.5.0",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.15.2",
    "foundation-sites": "^6.3.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^2.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.7",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "postcss": "^5.2.16",
    "react-styleguidist": "^4.6.3",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "webpack": "^1.14.0"
  }
}
